<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE component SYSTEM "dtds/component.dtd">

<!--
Family2 not have oid,
like the typical number/description table.
-->

<component name="Family2">

	<entity>
		<property name="number" type="int" key="true"
			size="3" required="true"/>   
		<property name="description" type="String"
			size="40" required="true"/>
	</entity>
					
	<entity-mapping table="XAVATEST@separator@FAMILY2">
		<property-mapping 
			property="number" column="NUMBER"/>
		<property-mapping
			property="description" column="DESCRIPTION"/>
	</entity-mapping> 
	
</component>